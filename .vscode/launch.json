{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Debug Server",
          "runtimeArgs": ["-r", "ts-node/register", "-r","tsconfig-paths/register"],
          "program": "${workspaceFolder}/src/index.ts",
          "sourceMaps": true,
          "envFile": "${workspaceFolder}/.env",
          "cwd": "${workspaceFolder}",
          "console": "internalConsole",
          "internalConsoleOptions": "openOnSessionStart",
          "outputCapture": "std"
      }
  ]
}
